<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(management) -%>">
  <span class='title'>
    <%= management.title %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), management_url(management),
        :title => t('.view_live'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_management_path(management),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_management_path(management),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :'data-confirm' => t('shared.admin.delete.message',
        :title => management.title
      ),
        :'data-method' => :delete %>
  </span>
</li>
